<?xml version="1.0" encoding="UTF-8"?>
<Daq>
	<DCC>
		<isMultiCast>true</isMultiCast>
		<isFemStuc>false</isFemStuc>
		<isAllCompress>true</isAllCompress>
		<requestedDataSize>65535</requestedDataSize>
		<FEM id="*">
			<isActive>true</isActive>
			<acqType range="{calibration, pedestal, standard}">standard
				<!-- {standard,calibration,pedestal} -->
			</acqType>
			<isChanAutoIncr>false</isChanAutoIncr>
			<nEvents>200</nEvents>
			<Module>
				<pulserDelay>0xD0</pulserDelay>
				<scaOffset range="[0x0, 0x1FF]">0x0</scaOffset>
				<pedestalActive>true</pedestalActive>
				<isCalculatedPedestal>true</isCalculatedPedestal>
				<pedestalMean>250</pedestalMean>
				<cmdRead range="{areq, dreq}">areq
					<!--{areq,dreq}-->
				</cmdRead>
				<readMode range="{space, time}">time
					<!--{space,time}-->
				</readMode>
				<compress>true</compress>
				<setSigmaThreshold>4.5</setSigmaThreshold>
				<isReadClockMask>true</isReadClockMask>
				<isWriteClockMask>false</isWriteClockMask>
				<divideScaClockBy>0x4</divideScaClockBy>
				<nTimeBuckets>0x1FF</nTimeBuckets>
				<nClocksBeforeStop>0x780</nClocksBeforeStop>
				<delayWriteReadClock>0x1FF</delayWriteReadClock>
			</Module>
			<FEC id="*">
				<isActive>true</isActive>
				<powerDown>false</powerDown>
				<Generator>
					<amplitude id="0" range="[0x0, 0xBFFF]">0xBFFF</amplitude>
					<amplitude id="1">0xBF00</amplitude>
					<amplitude id="2">0xBE00</amplitude>
					<amplitude id="3">0xBDC0</amplitude>
					<amplitude id="4">0xBDD0</amplitude>
					<amplitude id="5">0xBD00</amplitude>
					<tabMode>true</tabMode>
					<amplitudeStart range="[0x0, 0xBFFF]">0x3FFF</amplitudeStart>
					<amplitudeStop range="[0x0, 0xBFFF]">0x3DB8</amplitudeStop>
					<amplitudeIncr>0x40</amplitudeIncr>
				</Generator>
				<ASIC id="*">
					<isActive>true</isActive>
					<enableSlowControlCheck>false</enableSlowControlCheck>
					<doPACIgx2>true</doPACIgx2>
					<enableShaping>true</enableShaping>
					<gain unit="fC" range="{120, 240, 360, 600}">120
						<!-- 600fC,360fC,240fC,120fC -->
					</gain>
					<peakingTime unit="ns" range="{100, 200, 400, 500, 600, 700, 900, 1000, 1100, 1200, 1400, 1500, 1600, 1700, 1900, 2000}">100
						<!-- 100 ns; 200 ns; 400 ns; 500 ns; 600 ns;
			 		700 ns; 900 ns; 1000 ns; 1100 ns; 1200 ns;
			 		1400 ns; 1500 ns; 1600 ns; 1700 ns; 1900 ns; 2000 ns -->
					</peakingTime>
					<asicMode range="{calibration, functional, nothing, test}">nothing</asicMode>
					<!-- nothing ; calibration ; test ; functionnal -->
					<doMaxPowerWrite>false</doMaxPowerWrite>
					<doPowerDownRead>false</doPowerDownRead>
					<doAlternativePower>false</doAlternativePower>
					<reg2>0xA000</reg2>
					<channel id="*">
						<isActive>true</isActive>
						<isTestActive>true</isTestActive>
						<threshold>0x12C</threshold>
						<pedestal>0x30</pedestal>
						<transferFunctionSlope unit="ADC/fC">3.4</transferFunctionSlope>
						<transferFunctionAbscisse unit="ADC">12</transferFunctionAbscisse>
					</channel>
					<channel id="0" />
					<channel id="1" />
					<channel id="2" />
					<channel id="3" />
					<channel id="4" />
					<channel id="5" />
					<channel id="6" />
					<channel id="7" />
