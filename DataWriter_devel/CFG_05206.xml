				</ASIC>
				<ASIC id="0" />
				<ASIC id="1" />
				<ASIC id="2" />
				<ASIC id="3">
					<channel id="32" />
				</ASIC>
			</FEC>
			<FEC id="0" />
			<FEC id="1" />
			<FEC id="2" />
			<FEC id="3" />
			<FEC id="4" />
			<FEC id="5" />
		</FEM>
		<FEM id="0">
			<isActive>true</isActive>
			<acqType>standard</acqType>
		</FEM>
		<FEM id="1">
			<isActive>true</isActive>
			<acqType>standard</acqType>
