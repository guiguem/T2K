			<acqType>standard</acqType>
		</FEM>
		<FEM id="4">
			<acqType>standard
				<!-- {standard,calibration,pedestal} -->
			</acqType>
			<isActive>true</isActive>
		</FEM>
		<FEM id="5">
			<isActive>true</isActive>
			<acqType>standard</acqType>
		</FEM>
		<FEM id="6">
			<isActive>true</isActive>
			<acqType>standard</acqType>
		</FEM>
	</DCC>
	<Network>
		<IpNode id="*">
			<addrField id="0" range="[0, 255]">192</addrField>
			<addrField id="1">168</addrField>
			<addrField id="2">10</addrField>
			<addrField id="3">12</addrField>
			<port range="[1025, 32768]">1122</port>
		</IpNode>
		<IpNode id="LOCAL">
			<addrField id="3">11</addrField>
			<port>1123</port>
		</IpNode>
		<IpNode id="TARGET" />
		<replyTimeout unit="ms" range="[10, 10000]">5000</replyTimeout>
		<maxRetries range="[0, 10000]">5</maxRetries>
		<udpSocketRcvBufferSize unit="KB" range="[0x40, 0x4000]">0x100</udpSocketRcvBufferSize>
		<receiveBufferSize unit="KB" range="[0x100, 0x10000]">0x1000</receiveBufferSize>
	</Network>
	<Run>
		<Centrum>
			<isActive>false</isActive>
			<delay>0x0</delay>
		</Centrum>
		<maxEvents>2000</maxEvents>
		<dccServerLoopNumber>0</dccServerLoopNumber>
		<Trigger>
			<mode range="{external, internal}">external</mode>
			<Type range="{Beam_Spill, Cosmic, Laser, Test}">Test</Type>
			<waitforevent>0</waitforevent>
			<Latency range="[0, 65535]">0</Latency>
			<maxRetries>5000</maxRetries>
		</Trigger>
		<File>
			<maxSize unit="MB">0x3E8</maxSize>
		</File>
		<maxFiles>0</maxFiles>
	</Run>
</Daq>
